#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/ql/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/ql/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/ql/node_modules/.pnpm/semver@5.7.1/node_modules:/ql/node_modules/.pnpm/node_modules:/ql/node_modules:/node_modules:/ql/node_modules/.pnpm/jsonwebtoken@8.5.1/node_modules/jsonwebtoken/node_modules:/ql/node_modules/.pnpm/jsonwebtoken@8.5.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/ql/node_modules/.pnpm/semver@5.7.1/node_modules/semver/bin/node_modules:/ql/node_modules/.pnpm/semver@5.7.1/node_modules/semver/node_modules:/ql/node_modules/.pnpm/semver@5.7.1/node_modules:/ql/node_modules/.pnpm/node_modules:/ql/node_modules:/node_modules:/ql/node_modules/.pnpm/jsonwebtoken@8.5.1/node_modules/jsonwebtoken/node_modules:/ql/node_modules/.pnpm/jsonwebtoken@8.5.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@5.7.1/node_modules/semver/bin/semver" "$@"
else
  exec node  "$basedir/../../../../../semver@5.7.1/node_modules/semver/bin/semver" "$@"
fi
