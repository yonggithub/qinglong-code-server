#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/ql/node_modules/.pnpm/needle@2.9.1/node_modules/needle/bin/node_modules:/ql/node_modules/.pnpm/needle@2.9.1/node_modules/needle/node_modules:/ql/node_modules/.pnpm/needle@2.9.1/node_modules:/ql/node_modules/.pnpm/node_modules:/ql/node_modules:/node_modules:/ql/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/node_modules:/ql/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/ql/node_modules/.pnpm/needle@2.9.1/node_modules/needle/bin/node_modules:/ql/node_modules/.pnpm/needle@2.9.1/node_modules/needle/node_modules:/ql/node_modules/.pnpm/needle@2.9.1/node_modules:/ql/node_modules/.pnpm/node_modules:/ql/node_modules:/node_modules:/ql/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/node_modules:/ql/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../needle@2.9.1/node_modules/needle/bin/needle" "$@"
else
  exec node  "$basedir/../../../../../needle@2.9.1/node_modules/needle/bin/needle" "$@"
fi
